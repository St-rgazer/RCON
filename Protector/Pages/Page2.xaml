<Page x:Class="Protector.Pages.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Protector.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page2">

    <Grid>
        <TextBox x:Name="consoleBox" HorizontalAlignment="Center" Margin="0,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="790" Height="371" BorderBrush="{x:Null}" SelectionBrush="Black" Foreground="Gold" Background="{x:Null}" FontFamily="Consolas" IsEnabled="False"/>
        <TextBox x:Name="cmdBox" Margin="10,386,10,0" TextWrapping="Wrap" VerticalAlignment="Top" Background="#FF191919" Foreground="#FFBB86FC" BorderBrush="{x:Null}" Height="25" FontSize="18" FontFamily="Consolas" KeyDown="OnKeyDownHandler"/>
    </Grid>
</Page>
